#ifndef UNITY_SHADERGRAPH_MATH_BASIC
#define UNITY_SHADERGRAPH_MATH_BASIC

/****************************************************************

    Multiply

****************************************************************/
real Unity_Multiply(real a, real b)
{
    return a * b;
}

real2 Unity_Multiply(real2 a, real2 b)
{
    return a * b;
}

real3 Unity_Multiply(real3 a, real3 b)
{
    return a * b;
}

real4 Unity_Multiply(real4 a, real4 b)
{
    return a * b;
}

/****************************************************************

    Divide

****************************************************************/
real Unity_Divide(real a, real b)
{
    return a / b;
}

real2 Unity_Divide(real2 a, real2 b)
{
    return a / b;
}

real3 Unity_Divide(real3 a, real3 b)
{
    return a / b;
}

real4 Unity_Divide(real4 a, real4 b)
{
    return a / b;
}

#endif // UNITY_SHADERGRAPH_MATH_BASIC
